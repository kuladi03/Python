#include<iostream>
using namespace std;
class pass
{
public:
    char a[6]={'1','3','a','s','t','i'};
    int i ,j,k,l;
    void password()
    {
        for(i=0;i<6;i++)
        {
            for(j=0;j<6;j++)
            {
                for(k=0;k<6;k++)
                {
                    for(l=0;l<6;l++)
                    {
                        cout<<a[i]<<a[j]<<a[k]<<a[l]<<"\t";

                    }
                }
            }
        }
    }

};
int main()
{
    pass p;
    p.password();
    return 0;
}
